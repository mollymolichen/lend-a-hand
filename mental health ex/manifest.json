{
    "manifest_version": 2,

    "name": "HD Chrome Extension Example",
    "description": "Example extension for Hack Duke project",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "jquery-3.2.1.min.js",
                "content.js"
            ]
        }
    ],
    "permissions": [
        "activeTab"
    ]
}